#!/bin/bash
LOG_FILE="dpkg_server_logs.txt"
STUDENT_ID="123456"  # Replace with your actual student ID
# 1. Display all log entries
echo -e "\n$STUDENT_ID - All Log Entries:"
cat $LOG_FILE
# 2. Filter logs by date
read -p "Enter a date (YYYY-MM-DD): " date
echo -e "\n$STUDENT_ID - Log Entries for $date:"
grep "$date" $LOG_FILE
# 3. Filter logs by action type (install, upgrade, remove)
read -p "Enter action type (install/upgrade/remove): " action
echo -e "\n$STUDENT_ID - Log Entries for $action:"
grep "$action" $LOG_FILE
# 4. Summarize installed packages
echo -e "\n$STUDENT_ID - Installed Packages Summary:"
grep "install" $LOG_FILE | awk '{print $NF}' | sort | uniq -c | sort -nr
# 5. Summarize upgraded packages
echo -e "\n$STUDENT_ID - Upgraded Packages Summary:"
grep "upgrade" $LOG_FILE | awk '{print $NF}' | sort | uniq -c | sort -nr
# 6. Summarize removed packages
echo -e "\n$STUDENT_ID - Removed Packages Summary:"
grep "remove" $LOG_FILE | awk '{print $NF}' | sort | uniq -c | sort -nr
